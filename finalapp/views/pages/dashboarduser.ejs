                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="alert alert-success text-center">
                        <i class="fa fa-usd fa-3x"></i>&nbsp;<b><a href=""> Invest in Agritech Precision Farming </a></b> <br> to know more <a href=""> click here</a>  

                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="alert alert-info text-center">
                        <i class="fa fa-line-chart fa-3x"></i>&nbsp;<a href=""><b>Automate your Cloud Farm</a></b> <br> to know more <a href=""> click here</a>  


                    </div>
                </div>
               
            </div>
            <div class="row">
                <!-- Welcome -->
                <div class="col-lg-12">
                    <div class="alert alert-info">
                        <i class="fa fa-folder-open"></i><b>&nbsp;Hello ! </b>Welcome Back <b><%= username_dash  %> </b>
                    </div>
                </div>
                <!--end  Welcome -->
            </div>
            

            <div class="row">
                <div class="col-lg-8">



                    <!--Area chart example -->
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i>REMAINING TIME FOR NEXT CROP HARVEST
                            
                        </div>

                        <div class="panel-body">
                            <p id="demo" style="font-size: 36px;"><%= rdate  %> Days</p>

                        </div>
                    </div>
                    <!--End area chart example -->
                    <!--Simple table example -->
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i>SENSORS DATA STREAM TABLE
                            
                        </div>

                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="table-responsive">
                                        <table class="table table-bordered table-hover table-striped">
                                            <thead>
                                                <tr>
                                                    <th>Stream#</th>
                                                    <th>Date</th>
                                                    <th>Time</th>
                                                    <th>Temperature</th>
                                                    <th>Humidity</th>
                                                    <th>Soil Moisture</th>
                                                    <th>Light Intensity</th>
                                                    <th>chamberID</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% stream.forEach(function(item) { %>
                                                <tr>
                                                    <td> <%= item.stream_id%></td>
                                                    <td> <%= item.date%></td>
                                                    <td> <%= item.time%></td>
                                                    <td> <%= item.temperature%></td>
                                                    <td> <%= item.humidity%></td>
                                                    <td> <%= item.moisture%></td>
                                                    <td> <%= item.ldr%></td>
                                                    <td> <%= item.chamber_id%></td>
                                                    
                                                </tr>
                                                <% }); %>
                                               

                                            </tbody>
                                        </table>
                                    </div>

                                </div>

                            </div>
                            <!-- /.row -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!--End simple table example -->

                </div>
                <div class="col-lg-4">
                    <!-- Notifications-->
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <i class="fa fa-bell fa-fw"></i>Activity Log
                        </div>

                        <div class="panel-body">
                            <div class="list-group">
                                <a href="#" class="list-group-item">
                                    <i class="fa fa-comment fa-fw"></i>Cooling down the Atmosphere
                                    <span class="pull-right text-muted small"><em>4 minutes ago</em>
                                    </span>
                                </a>
                                <a href="#" class="list-group-item">
                                    <i class="fa fa-warning fa-fw"></i>temperature High alert
